<?xml version="1.0" encoding="utf-8"?>

<odoo>

  <record id="user_sales_status_id" model="ir.ui.view">
      <field name="name">user_sales_status_id</field>
      <field name="model">res.users</field>
      <field name="inherit_id" ref="base.view_users_form"/>
      <field name="arch" type="xml">
          <xpath expr="//page[@name='preferences']" position="after">
             <page string="Real Estate Property" name="preferences">
                  <field name="property_ids" nolabel="1" delete="false"/>
             </page>
            </xpath>
      </field>
  </record>
</odoo>